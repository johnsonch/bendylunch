.afs-ubuntu { font-family: "Ubuntu", "Arial", "Helvetica", "Lucida Grande", "Verdana", "Gill Sans", sans-serif; ;}

.afs-pt-serif { font-family: "PT Serif", "Times New Roman", "Georgia", "Palatino", "Hoefler Text", "Baskerville", serif; font-size: 1.040em;}

@font-face {
  font-family: 'Ubuntu';
  src: url(<%= asset_path('Ubuntu-B-webfont.eot') %>);
  src: local('☺'), url(<%= asset_path('Ubuntu-B-webfont.woff') %>) format('woff'), url(<%= asset_path('Ubuntu-B-webfont.ttf') %>) format('truetype'), url(<%= asset_path('Ubuntu-B-webfont.svg#webfontX4pGz9fc') %>) format('svg');
  font-weight: 700;
  font-style: normal;
}


